    "def check_stationarity(series):\n",
    "    result = adfuller(series.dropna())\n",
    "    p_value = result[1]\n",
    "    print(f\"ADF test for {series.name}: p-value = {p_value:.4f}\")\n",
    "    return p_value"
   ]
